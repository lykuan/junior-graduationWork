<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-ytp" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>ytp</name>
    <js-module name="ytp" src="www/ytp.js">
        <clobbers target="cordova.plugins.ytp" />
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="ytp">
                <param name="android-package" value="cordova-plugin-ytp.ytp" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/ytp.java" target-dir="src/cordova-plugin-ytp/ytp" />
    </platform>
</plugin>